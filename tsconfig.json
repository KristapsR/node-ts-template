{
  "compilerOptions": {
    "target": "ES2021",
    "module": "Node16",
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "moduleResolution": "Node16",
    "outDir": "dist",
    "pretty": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "importHelpers": true
  }
}
